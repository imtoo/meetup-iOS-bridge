{
  "name": "meetup",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "app:ios": "yarn kill:packager && (concurrently \"yarn start\" \"sleep 5 && yarn ios\")",
    "kill:packager": "kill-port --port 8081",
    "ios": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.3"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "jest": "22.4.3",
    "kill-port": "^1.1.0",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
